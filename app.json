{
  "expo": {
    "name": "flinch",
    "slug": "flinch-me",
    "icon": "./assets/images/logo.png",
    "version": "1.2",
    "splash": {
      "image": "./assets/images/splash5.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*",
      "assets/lottie/*",
      "node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/MaterialCommunityIcons.tff"
    ],
    "android":{
      "package": "com.shoutouts.flinch",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.shoutouts.flinch",
      "buildNumber": "1.2.0",
      "requireFullScreen": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}