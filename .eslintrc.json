{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "no-use-before-define": 0,
    "camelcase": 0,
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
    "no-extra-boolean-cast": 0,
    "multiline-ternary": 0,
    "space-before-function-paren": 0,
    "indent": 0
  }
}
